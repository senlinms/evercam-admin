<!-- BEGIN CONTENT -->
<div class="page-content-wrapper">
  <div class="page-content" id="cameras-report">
    <div class="row overlop-margin">
      <div class="dropdown-checklist" id="div-dropdown-checklist">
        <div href="#" class="btn btn-default grey" data-toggle="modal" data-target="#toggle-datatable-columns">
          <i class="fa fa-columns"></i>
        </div>
      </div>
    </div>
    <%= render 'columns_modal' %>
    <!-- BEGIN PAGE CONTENT-->
    <div id="merge-list-row" class="row dt-user-m">
      <div class="col-md-12">
        <table class="table table-striped table-bordered table-hover archive-datatables" id="extractor_datatables">
          <thead>
            <tr role="row" class="heading">
              <th name="order_id" class="sorting">ID</th>
              <th name="order_camera" class="sorting">Camera</th>
              <th name="order_from_date" class="sorting">From Date</th>
              <th name="order_to_date" class="sorting">To Date</th>
              <th name="order_interval" class="sorting">Interval</th>
              <th name="order_status" class="sorting">Status</th>
              <th name="order_notes" class="sorting">Notes</th>
              <th name="order_extraction" class="sorting">Extraction</th>
              <th name="order_requestor" class="sorting">Requestor</th>
              <th name="order_created_at" class="sorting">Created At</th>
            </tr>
          </thead>
          <tbody>
            <%= render "extractor" %>
          </tbody>
        </table>
      </div>
    <!-- END PAGE CONTENT-->
  </div>
  </div>
</div>
<!-- END CONTENT -->
<script>
  $(document).ready(function () {
    window.initializeExtractors();
  });
</script>
