<%= content_for(:title, "Duplicate Cameras") %>
<!-- BEGIN CONTENT -->
<div class="page-content-wrapper">
  <div class="page-content" id="cameras-report">
    <%= render "merge_modal" %>
    <!-- BEGIN PAGE CONTENT-->
    <div class="row overlapping">
      <div class="form-merge-report">
        <div class="main-contains">
          <div class="filter-naming">
            Status :
          </div>
          <div class="inputs-divs">
            <input type="text" class="first-inps" placeholder="Search IP" id="camera-ip">
          </div>
        </div>
      </div>
    </div>
    <div id="merge-list-row" class="row dt-user-m">
      <div class="col-md-12">
        <table class="table table-striped table-bordered table-hover" id="merge_datatables">
          <thead>
            <tr role="row" class="heading">
              <th name="order_camera_ip" class="sorting">Camera IP</th>
              <th name="order_http_port" class="sorting">Http Port</th>
              <th name="order_snapshot_url" class="sorting">Snapshot url</th>
              <th name="order_cr_status" class="sorting">CR Status</th>
              <th name="order_online" class="sorting">Online</th>
              <th name="order_count" class="sorting">Count</th>
              <th name="order_action" class="sorting">Action</th>
            </tr>
          </thead>
          <tbody>
            <%=  render "merge" %>
          </tbody>
        </table>
      </div>
    <!-- END PAGE CONTENT-->
  </div>
  </div>
</div>
<!-- END CONTENT -->

<script>
  $(document).ready(function () {
    window.initializeMerges();
  });
</script>